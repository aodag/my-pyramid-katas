{% extends main_template %}
{% block main_contents %}
<table class="table">
  <thead>
    <tr>
      <th>ID</th>
      <th>username</th>
    </tr>
  </thead>
  <tbody>
    {% for user in users %}
    <tr>
      <td>{{ user.id }}</td>
      <td>{{ user.username }}</td>
    </tr>
    {% endfor %}
  </tbody>
</table>
{% endblock %}
